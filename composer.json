{
  "name": "santosdave/amadeus-enterprise",
  "version": "1.0.0",
  "type": "library",
  "description": "SOAP Web Service client library for interacting with the Amadeus GDS through its SOAP interface",
  "keywords": ["amadeus", "gds", "service", "webservice", "soap", "wsdl", "client", "xml", "travel", "airline", "hotel", "booking", "car", "rental", "offer", "masterpricer"],
  "minimum-stability": "stable",
  "homepage": "https://github.com/santosdave/amadeus-enterprise",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Wycliffe Dev",
      "email": "santosdave86@gmail.com",
      "homepage": "http://dermika.be",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-xsl": "*",
    "psr/log": "*"
    
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Amadeus\\": "src/Amadeus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Amadeus\\": "tests/Amadeus"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}
